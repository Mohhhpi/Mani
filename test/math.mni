load "munit";
load "math";

let t = newTester();
let x = 1.23;

t.header("Math");

t.assertEquals(floor(x), 1, "Test floor");

t.assertEquals(ceil(x), 2, "Test ceil");

t.assertEquals(asin(1), 1.5707963267948966, "Test asin");

t.assertEquals(acos(1), 0.0, "Test acos");

t.assertEquals(atan(1), 0.7853981633974483, "Test atan");

t.assertEquals(atan2(x, x), 0.7853981633974483, "Test atan2");

t.assertEquals(cbrt(x), 1.0714412696907731, "Test cbrt");

t.assertEquals(cos(x), 0.3342377271245026, "Test cos");

t.assertEquals(sin(x), 0.9424888019316975, "Test sin");

t.assertEquals(tan(x), 2.819815734268152, "Test tan");

t.assertEquals(cosh(x), 1.8567610569852664, "Test cosh");

t.assertEquals(exp(x), 3.4212295362896734, "Test exp");

t.assertEquals(expm1(x), 2.4212295362896734, "Test expm1");

t.results();
